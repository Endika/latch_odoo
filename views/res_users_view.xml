<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="res_partner_form_inherit" model="ir.ui.view">
            <field name="name">res.users.form.latch</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form_simple_modif"/>
            <field name="arch" type="xml">

                <field name="image" position="after">
                    <div>
                    <button name="action_latch_wizard" string="Latch" type="object"/>
                    </div>
                </field>

            </field>
        </record>

        <record id="wizard_latch_pare" model="ir.ui.view">
            <field name="name">latch.pare</field>
            <field name="model">latch.odoo</field>
            <field name="arch" type="xml">
                <form string="Latch Odoo" version="7.0">
                    HOla
                    <field name="code"/>

                <footer>
                    <button special="cancel" string="Close" type="object"/>
                </footer>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_wizard_latch_odoo">
            <field name="name">Latch Odoo</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">latch.odoo</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</openerp>
